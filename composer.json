{
  "name": "m-hume/qutee",
  "type": "library",
  "description": "Simple queue manager and task processor for PHP",
  "keywords": ["qutee", "job", "task", "worker", "background", "queue", "redis", "mysql", "beanstalkd"],
  "homepage": "https://github.com/m-hume/qutee",
  "authors": [
    {"name": "Marin CrnkoviÄ‡", "email": "marin.crnkovic@gmail.com"}
    {"name": "mhume"}
  ],
  "license": "LGPL-2.1+",

  "minimum-stability": "dev",

  "require": {
    "php": ">=5.3.3",
    "symfony/event-dispatcher": "~2.7"
  },

  "require-dev": {
    "phpunit/phpunit": "~4.6",
    "satooshi/php-coveralls": "dev-master"
  },

  "suggest": {
    "pda/pheanstalk": "3.x-dev - for beanstalkd persistor",
    "predis/predis": "Might be needed, depending on the chosen persistor (no need to compile phpredis)",
    "ext-phpredis": "Might be needed, depending on the chosen persistor (faster than predis)"
  },

  "autoload": {
    "psr-0": {
      "Qutee": "src/"
    }
  }
}
